{% extends 'base.html.twig' %}

{% block title %}Triprequest{% endblock %}

{% block body %}
    <h1>Triprequest</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ triprequest.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ triprequest.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ triprequest.email }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ triprequest.phone }}</td>
            </tr>
            <tr>
                <th>People_number</th>
                <td>{{ triprequest.peopleNumber }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ triprequest.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Processed</th>
                <td>{{ triprequest.processed ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_triprequest_index') }}">back to list</a>

    <a href="{{ path('app_triprequest_edit', {'id': triprequest.id}) }}">edit</a>

    {{ include('triprequest/_delete_form.html.twig') }}
{% endblock %}
